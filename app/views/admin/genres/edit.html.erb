<div class="breadcrumb-navigation">
  <i class="fa fa-angle-right">&nbsp;</i>
  <span>HOME<span>
  <i class="fa fa-angle-right">&nbsp;</i>
  <span>GENRE<span>
  <i class="fa fa-angle-right">&nbsp;</i>
  <span>INDEX<span>
  <i class="fa fa-angle-right">&nbsp;</i>
  <span>EDIT<span>

<div class="PageHead_practator">
  <div class="PageHead_practator-title">
    <h2>GENRE</h2>
  </div>
  <div class="PageHead_practator-text">
    <h6>記事のジャンルの編集管理画面</h6>
  </div>
</div>
<div class="signup-display">
  <%= form_with model:@genre, url: admin_genre_path(@genre), local:true do |f| %>
    <div class="user_edit-display_form">
      <div class="form-group form-inline">
        <%= f.label :name, 'ジャンル名', class:"col-sm-2 control-label font-weight-lighter small" %>
        <%= f.text_field :name, autofocus: true, autocomplete: "name", placeholder: "", class:"form-control col-sm-4" %>
      </div>
      <div class="signup-button">
        <%= f.submit "編集完了", class:"btn btn-secondary col-sm-6" %>
      </div>
    </div>
  <% end %>
</div>